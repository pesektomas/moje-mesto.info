{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"browsers": [
						"last 2 versions"
					]
				},
				"useBuiltIns": "usage"
			}
		],
		[
			"@babel/preset-react"
		]
	],
	"plugins": [
		[
			"react-intl"
		],
		[
			"@babel/plugin-proposal-class-properties",
			{
				"spec": true
			}
		],
		[
			"emotion"
		]
	]
}
